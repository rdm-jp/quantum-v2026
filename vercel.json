{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"  
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"  
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,       
      "maxDuration": 10     
    }
  },
  "fluid": true           
}